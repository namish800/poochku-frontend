{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/login",
            "redirect": "/.auth/login/aad"
        },
        {
            "route": "/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html"
    }
}